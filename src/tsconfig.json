{
"compilerOptions": {
  "target": "es6",
  "strictPropertyInitialization": false,
  "module": "commonjs",
  "incremental": true,
  "removeComments": true,
  "noImplicitAny": false,
  "noImplicitThis": true,
  "noUnusedLocals": true,
  "esModuleInterop": true,
  "moduleResolution": "node",
  "emitDecoratorMetadata": true,
  "experimentalDecorators": true,
  "declaration": false,
  "noResolve": false,
  "baseUrl": "src/bot",
  "outDir": "../dest",
  "inlineSourceMap": true,
  "inlineSources": true,
  "skipLibCheck": true,
  },
  "exclude": [
    "node_modules"
  ]
}