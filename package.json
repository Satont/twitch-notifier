{
  "name": "twitch-notifier",
  "version": "4.0.0",
  "description": "Notify users about some streamer wen't online",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "npm run migration:up",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "eslint": "npx eslint --ext .ts src",
    "eslint:fix": "npx eslint --ext .ts --fix src",
    "migration:create": "npx typeorm migration:generate -p -n",
    "migration:up": "typeorm migration:run",
    "dev": "nodemon",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satont/twitch-notifier.git"
  },
  "keywords": [
    "vk",
    "telegram",
    "twitch",
    "notify"
  ],
  "author": "Satont <satontworldwide@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Satont/twitch-notifier/issues"
  },
  "homepage": "https://github.com/Satont/twitch-notifier#readme",
  "dependencies": {
    "@nestjs/common": "^7.5.5",
    "@nestjs/core": "^7.5.5",
    "@nestjs/platform-express": "^7.5.5",
    "@nestjs/schematics": "^7.2.2",
    "axios": "^0.21.0",
    "dayjs": "^1.9.3",
    "dotenv": "^8.2.0",
    "i18next": "^19.8.4",
    "i18next-fs-backend": "^1.0.7",
    "lodash": "^4.17.20",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rotating-file-stream": "^2.1.3",
    "source-map-support": "^0.5.19",
    "telegraf": "^3.38.0",
    "twitch": "^4.3.2",
    "typeorm": "^0.2.29",
    "vk-io": "^4.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.3",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.10",
    "@types/source-map-support": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.11.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dest/mikro-orm.config.js"
    ]
  }
}
