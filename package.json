    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.0",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "telegraf": "^3.33.0",
    "vk-io": "^4.0.0-rc.22"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/cors": "^2.8.6",
