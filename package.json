{
  "name": "twitch-notifier",
  "version": "4.1.0",
  "description": "Notify users about some streamer wen't online",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "pnpm run migration:up",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "eslint": "npx eslint --ext .ts src",
    "eslint:fix": "npx eslint --ext .ts --fix src",
    "migration:create": "npx typeorm migration:generate -p -n",
    "migration:up": "typeorm migration:run",
    "dev": "nodemon",
    "debug": "node --inspect=0.0.0.0:9229 --nolazy ./dist/index.js",
    "start": "node dist/index.js",
    "twitch:event": "ts-node tools/emitTwitchEvent.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satont/twitch-notifier.git"
  },
  "keywords": [
    "telegram",
    "twitch",
    "notify"
  ],
  "author": "Satont <satontworldwide@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Satont/twitch-notifier/issues"
  },
  "homepage": "https://github.com/Satont/twitch-notifier#readme",
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/schematics": "^7.3.1",
    "@sentry/node": "^5.30.0",
    "@twurple/api": "^5.0.13",
    "@twurple/auth": "^5.0.13",
    "@twurple/eventsub": "^5.0.13",
    "axios": "^0.21.4",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dayjs": "^1.10.7",
    "dotenv": "^8.6.0",
    "glob": "^7.2.0",
    "hbs": "^4.2.0",
    "lodash": "^4.17.21",
    "nestjs-pino": "^1.4.0",
    "pg": "^8.7.1",
    "pino-pretty": "^4.8.0",
    "reflect-metadata": "^0.1.13",
    "rotating-file-stream": "^2.1.6",
    "source-map-support": "^0.5.21",
    "strip-ansi": "^6.0.0",
    "telegraf": "^3.39.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/hbs": "^4.0.1",
    "@types/localtunnel": "^2.0.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.5",
    "@types/source-map-support": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.5.4"
  }
}
