    "eslint": "^7.17.0",
    "jest": "^27.1.0",
    "localtunnel": "^2.0.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
