{
  "name": "twitch-notifier",
  "version": "4.1.0",
  "description": "Notify users about some streamer wen't online",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "prestart": "npm run migration:up",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "eslint": "npx eslint --ext .ts src",
    "eslint:fix": "npx eslint --ext .ts --fix src",
    "migration:create": "npx typeorm migration:generate -p -n",
    "migration:up": "typeorm migration:run",
    "dev": "nodemon",
    "debug": "node --inspect=0.0.0.0:9229 --nolazy ./dist/index.js",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satont/twitch-notifier.git"
  },
  "keywords": [
    "vk",
    "telegram",
    "twitch",
    "notify"
  ],
  "author": "Satont <satontworldwide@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Satont/twitch-notifier/issues"
  },
  "homepage": "https://github.com/Satont/twitch-notifier#readme",
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/core": "^7.6.5",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/schematics": "^7.2.6",
    "@sentry/node": "^5.29.2",
    "@vk-io/hear": "^1.0.2",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dayjs": "^1.10.5",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "hbs": "^4.1.1",
    "lodash": "^4.17.20",
    "nestjs-pino": "^1.3.0",
    "pg": "^8.5.1",
    "pino-pretty": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "rotating-file-stream": "^2.1.3",
    "source-map-support": "^0.5.19",
    "telegraf": "^3.38.0",
    "twitch": "^4.3.8",
    "twitch-eventsub": "^4.5.5",
    "typeorm": "^0.2.29",
    "vk-io": "^4.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
